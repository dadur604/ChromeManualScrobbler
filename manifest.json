{
	"manifest_version": 2,
	"name": "ManualScrobbler",
	"version": "1.1",
	"description": "Manual Last.fm scrobbler that auto-fills on youtube videos.",

	"browser_action": {
		"browser_style": true,
		"default_popup": "popup/index.html",
		"default_icon": {
			"16": "icons/icon_16.png",
			"32": "icons/icon_32.png",
			"48": "icons/icon_48.png"
		}
	},

	
	"permissions": [
    "tabs",
    "declarativeContent",
	"webRequest",
	"webRequestBlocking",
	"storage",
	"<all_urls>"
  ]

}